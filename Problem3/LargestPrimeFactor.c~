#include<stdio.h>
#include<time.h>
#include<math.h>

int CheckPrime(int i){

  int j = 0;
  int i_sqrt = sqrt(i);
  
  for(j=3; j <= i_sqrt; j+=2){
    if(i%j == 0) reurn 0;
  } 
  return 1;
}

int main(){
  int i, j;

  if(CheckPrime(i) == 1) printf("%d is prime", i);
  else printf("%d is not prime", i);

}
